{
  "buildCommand": "cd frontend && npm install --production=false && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "echo 'Skipping workspace install'",
  "framework": "vite",
  "env": {
    "CI": "true",
    "NODE_ENV": "production",
    "SKIP_PREPARE": "true"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}